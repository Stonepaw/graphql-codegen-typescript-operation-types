{
  "name": "graphql-codegen-typescript-operation-types",
  "version": "1.1.4",
  "description": "GraphQL codegen plugin that generates only the types used in the operations",
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "repository": "https://github.com/Stonepaw/graphql-codegen-typescript-operation-types",
  "license": "MIT",
  "keywords": [
    "graphql",
    "codegen",
    "graphql-codegen",
    "gql",
    "typescript"
  ],
  "scripts": {
    "test": "jest --forceExit --no-watchman",
    "lint": "eslint src --ext .ts",
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.prod.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "watch:build": "tsc -p tsconfig.prod.json -w",
    "watch:test": "jest --watch"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@graphql-codegen/typescript": "^2.4.8"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "^1.17.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.18.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "graphql": "^16.0.0",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "files": [
    "dist/main",
    "dist/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
